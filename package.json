{
    "name": "pilotestudio",
    "version": "1.0.0",
    "description": "Bip boup ! Je suis le bot OpenSource de Pilote Production.",
    "main": "src/main.js",
    "type": "module",
    "scripts": {
      "start": "node src/main.js",
      "dev": "nodemon src/main.js",
      "deploy": "node src/deploy-commands.js",
      "deploy:dev": "node src/deploy-commands.js --dev",
      "lint": "eslint src/**/*.js",
      "format": "prettier --write src/**/*.js",
      "backup-db": "node src/utils/backup-database.js",
