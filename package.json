    "keywords": [
      "discord",
      "bot",
      "economy",
      "sqlite"
    ],
    "author": "Pilote Production",
    "license": "MIT",
    "dependencies": {
      "discord.js": "^14.14.1",
      "dotenv": "^16.3.1",
      "sqlite": "^5.0.1",
      "sqlite3": "^5.1.6"
    },
    "devDependencies": {
      "eslint": "^8.56.0",
      "jest": "^29.7.0",
      "nodemon": "^3.0.2",
      "prettier": "^3.1.1"
    },
    "engines": {
      "node": ">=18.0.0"
    }
  }